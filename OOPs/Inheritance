class Animal{
        void eat(){
            System.out.println("i can eat");
        }
    }
    class Dog extends Animal{
         void bark(){
             System.out.println("i can bark");
         }
     }
public class Main{
    public static void main (String args[]){
       Dog d = new Dog();
       d.eat();
       d.bark();
    
}

    }
